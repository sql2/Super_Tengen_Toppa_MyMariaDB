    "172.16.20.1",
    "172.16.20.2",
    "172.16.20.3"
  ],
  "ui"                   : false,
  "leave_on_terminate"   : true,
  "enable_script_checks" : true,
  "disable_update_check" : true,
  "watches": [
    {   
      "type" : "event",
      "name" : "deploy",
      "args" : ["/root/provisioning.sh"]
    }  
  ],
  "service" : {   
        "id"       : "mariadb-cluster",
        "name"     : "mariadb-cluster",
        "address"  : "",
        "tags" : [ 
           "linux",
           "mariadb"
        ],
        "meta" : {
           "meta"  : "You are a meta." 
        }
  }
